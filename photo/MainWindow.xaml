<Window x:Class="photo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:photo"
        mc:Ignorable="d"
       Title="Photo Album" Height="450" Width="800">
    <Grid>
        <TabControl Name="tabControl">
            <!-- Пример вкладки. Добавьте свои вкладки аналогичным образом -->
            <TabItem Header="Photo1">
                <Image Name="photo1" Source="/image 650.jpg" Stretch="Uniform"/>
            </TabItem>
            <TabItem Header="Photo2">
                <Image Name="photo2" Source="/image 654.jpg" Stretch="Uniform"/>
            </TabItem>
            <TabItem Header="Photo3">
                <Image Name="photo3" Source="/image 655.jpg" Stretch="Uniform"/>
            </TabItem>
            <TabItem Header="Photo4">
                <Image Name="photo4" Source="/image 656.jpg" Stretch="Uniform"/>
            </TabItem>
            <!-- Добавьте другие вкладки аналогичным образом -->
        </TabControl>
    </Grid>
</Window>
